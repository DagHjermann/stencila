{
  "name": "stencila",
  "version": "0.25.0",
  "description": "Documents powered by data",
  "main": "index.js",
  "scripts": {
    "prepublish": "node_modules/.bin/rollup --config rollup-config.browser.js",
    "lint": "eslint *.js src tests",
    "test": "tape --require babel-register tests | tap-spec",
    "test-bundle": "rollup --watch --config rollup-config.tests.js",
    "build": "node_modules/.bin/rollup --config rollup-config.browser.js",
    "watch": "node_modules/.bin/rollup --watch --config rollup-config.browser.js",
    "cover": "babel-node ./node_modules/.bin/babel-istanbul cover tests",
    "docs": "documentation build --config docs/docs.yml --output docs --format html",
    "docs-serve": "documentation serve --config docs/docs.yml --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/stencila.git"
  },
  "keywords": [
    "reproducibility",
    "data",
    "science",
    "documents",
    "sheets",
    "contexts"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stencila/stencila/issues"
  },
  "homepage": "https://github.com/stencila/stencila#readme",
  "dependencies": {
    "d3": "^4.4.0",
    "font-awesome": "^4.6.3",
    "katex": "^0.6.0",
    "semantic-ui-css": "^2.2.4",
    "substance": "substance/substance#kaikoura",
    "substance-mini": "0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "brace": "^0.9.1",
    "browserify": "^14.1.0",
    "documentation": "^4.0.0-beta16",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-hypothetical": "^1.2.1",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "sanitize-html": "^1.13.0",
    "sprintf-js": "^1.0.3",
    "substance-bundler": "^0.12.2",
    "substance-test": "^0.8.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "uglify-js-harmony": "^2.7.5"
  }
}
