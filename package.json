{
  "name": "stencila-js",
  "version": "0.4.0",
  "description": "Stencila components for Javascript",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src tests",
    "test": "tape --require babel-register tests/**/*.test.js | tap-spec",
    "test-bundle": "rollup --watch --config tests/rollup-config.js",
    "test-documents": "rm -rf tmp && mkdir -p tmp && if [ ! -d 'tmp/stencila' ]; then git clone --depth=1 https://github.com/stencila/stencila tmp/stencila; fi && ln -sf `pwd`/tmp/stencila/tests/documents tests/document/documents",
    "cover": "istanbul cover tests",
    "docs": "documentation build --config docs/docs.yml --output docs --format html",
    "docs-serve": "documentation serve --config docs/docs.yml --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/js.git"
  },
  "keywords": [
    "data",
    "documents",
    "sheets",
    "contexts",
    "sessions",
    "reproducibility",
    "science"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stencila/js/issues"
  },
  "homepage": "https://github.com/stencila/js#readme",
  "dependencies": {
    "buble": "^0.15.2",
    "cheerio": "^0.22.0",
    "d3": "^4.4.0",
    "github-api": "^3.0.0",
    "hast-util-to-string": "^1.0.0",
    "js-beautify": "^1.6.8",
    "rehype": "3.0.0",
    "rehype-minify-whitespace": "^1.1.0",
    "rehype-parse": "2.0.1",
    "rehype-remark": "^1.1.0",
    "rehype-stringify": "^2.1.0",
    "remark-bracketed-spans": "^2.1.0",
    "remark-html": "5.2.0",
    "remark-parse": "2.2.0",
    "remark-slug": "^4.2.2",
    "remark-squeeze-paragraphs": "^3.0.0",
    "remark-stringify": "2.4.0",
    "request": "^2.79.0",
    "unified": "5.1.0",
    "unist-builder": "^1.0.2",
    "unist-util-find-after": "^2.0.0",
    "unist-util-remove": "^0.2.1",
    "unist-util-visit": "^1.1.1",
    "vega": "^3.0.0-beta.22",
    "vega-lite": "^2.0.0-alpha.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "documentation": "^4.0.0-beta16",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "substance-test": "^0.6.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "testling": "^1.7.1"
  }
}
