{
  "name": "stencila-js",
  "version": "0.4.0",
  "description": "Stencila components for Javascript",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src tests",
    "test": "tape --require babel-register tests | tap-spec",
    "test-bundle": "rollup --watch --config rollup-config.tests.js",
    "build": "node_modules/.bin/rollup --config rollup-config.browser.js",
    "watch": "node_modules/.bin/rollup --watch --config rollup-config.browser.js",
    "cover": "babel-node ./node_modules/.bin/babel-istanbul cover tests",
    "docs": "documentation build --config docs/docs.yml --output docs --format html",
    "docs-serve": "documentation serve --config docs/docs.yml --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/js.git"
  },
  "keywords": [
    "data",
    "documents",
    "sheets",
    "contexts",
    "sessions",
    "reproducibility",
    "science"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stencila/js/issues"
  },
  "homepage": "https://github.com/stencila/js#readme",
  "dependencies": {
    "d3": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "documentation": "^4.0.0-beta16",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-hypothetical": "^1.2.1",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "substance-test": "^0.8.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
