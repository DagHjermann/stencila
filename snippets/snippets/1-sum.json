{
	"name": "sum",
	"summary": "Returns the sum of values",
	"notes": [
		"If only a single number for `values` is supplied, `sum` returns `values`"
	],
	"examples": [
		"sum(A1)",
		"sum(A1:A10)",
		"sum(A1:G10)"
	],
	"see": [
		"sumsq",
		"sumif",
		"product"
	],
	"languages": [
		"r",
		"py"
	],
	"parameters": [
		{
			"name": "values",
			"description": "Number or cell or range of cells to sum up"
		} 
	],
	"code": [
		{
			"language": "r",
			"call": "sum"
		},
		{
			"language": "py",
			"subs": "sum(${0})"
		},
		{
			"language": "jl",
			"expr": "'sum(' + exprs[0] + ')'"
		},
		{
			"language": "hs",
			"func": "function(pars){ return 'sum(' + pars[0].expr + ')'; }"
		}
	]
}
