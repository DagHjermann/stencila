{
	"id": "scatterplot",
	"summary": "A simple scatterplot of two variables",
	"notes": [
		"The length of `x` and `y` must be the same"
	],
	"examples": [
		"scatterplot(A1:A40,B1:B40,xlab='Temperature',y='Ice cream sales')"
	],
	"see": [
	],
	"languages": [
		"r"
	],
	"parameters": [
		{
			"name": "x",
			"description": "X values"
		},
		{
			"name": "y",
			"description": "Y values"
		},
		{
			"name": "xlab",
			"description": "X axis label"
		},
		{
			"name": "ylab",
			"description": "Y axis label"
		}
	],
	"code": [
		{
			"language": "r",
			"expr": "'plot(' + exprs[0] + ',' + exprs[1] + exprs[2]?(',xlab='+exprs[2]):'' + exprs[3]?(',ylab='+exprs[3]):'' + ')'"
		}
	]
}
